<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Detail Project</title>

    <link rel="stylesheet" href="/css/personalweb.css" />

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
  </head>

  <body>
    <nav class="navbar">
      <div class="container">
        <a class="btn btn-dark navbar-brand" href="/">Home</a>
        <a class="btn btn-dark" href="/my-project">My Project</a>
        <a class="btn btn-dark" href="/contact">Contact</a>
      </div>
    </nav>

    <section class="webpage section">
      <div class="container">
        <h2>{{project.name}}</h2>

        <p>
          <strong>Duration:</strong><br />
          {{project.startDate}}
          -
          {{project.endDate}}
        </p>

        <p>{{project.description}}</p>

        <a href="/my-project" class="btn btn-dark btn-sm">
          Back
        </a>
      </div>
    </section>
  </body>
</html>